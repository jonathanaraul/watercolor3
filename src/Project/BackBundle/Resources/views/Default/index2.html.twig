{% extends '::base.html.twig' %}

{% block pagesScriptsBottom %}

<script src="{{asset('admin/js/helpers/grafico-social.js')}}"></script>
<script src="{{asset('admin/js/helpers/grafico-dominios.js')}}"></script>
<script src="{{asset('admin/js/helpers/widget-reciente.js')}}"></script>
<script src="{{asset('admin/js/helpers/widget-comentarios.js')}}"></script>
{%endblock%}

{% block pageContent  %}
<div class="main-container container-fluid" id="sinlogin">
	<a class="menu-toggler" id="menu-toggler" href="#"> <span class="menu-text"></span> </a>

	{% include '::sidebar.html.twig'  with {'clase': 'gris'}  %}

	<div class="main-content">

		{# include 'ProjectBackBundle:Architecture:breadcrumb.html.twig'  with {'clase': 'gris'} #}
		<div class="page-content">

			{% include 'ProjectBackBundle:Helpers:page-header.html.twig'  with {'titulo': 'Panel de control','subtitulo':'Vista de elementos'}  %}

			<div class="row-fluid">
				<div class="span12">
					<!--PAGE CONTENT BEGINS-->

					<div class="alert alert-block alert-success">
						<button type="button" class="close" data-dismiss="alert">
							<i class="icon-remove"></i>
						</button>

						<i class="icon-ok green"></i>

						Welcome to
						<strong class="green"> Ace <small>(v1.1.2)</small> </strong>
						,
						the lightweight, feature-rich and easy to use admin template.
					</div>

					<div class="space-6"></div>

					<div class="row-fluid">
						{% include 'ProjectBackBundle:Helpers:infobox.html.twig'  with {'clase': 'gris'}  %}

						<div class="vspace"></div>

						<div class="span5">
							{% include 'ProjectBackBundle:Helpers:grafico-trafico.html.twig'  with {'clase': 'gris'}  %}
						</div><!--/span-->
					</div><!--/row-->

					<div class="hr hr32 hr-dotted"></div>

					<div class="row-fluid">
						<div class="span5">
							{% include 'ProjectBackBundle:Helpers:tabla-dominios.html.twig'  with {'clase': 'gris'}  %}
						</div>

						<div class="span7">
							{% include 'ProjectBackBundle:Helpers:grafico-dominios.html.twig'  with {'clase': 'gris'}  %}
						</div>
					</div>

					<div class="hr hr32 hr-dotted"></div>

					<div class="row-fluid">
						<div class="span6">
							{% include 'ProjectBackBundle:Helpers:widget-reciente.html.twig'  with {'clase': 'gris'}  %}
						</div><!--/span-->

						<div class="span6">
							{% include 'ProjectBackBundle:Helpers:widget-comentarios.html.twig'  with {'clase': 'gris'}  %}
						</div><!--/span-->
					</div><!--/row-->

					<!--PAGE CONTENT ENDS-->
				</div><!--/.span-->
			</div><!--/.row-fluid-->
		</div><!--/.page-content-->
	</div><!--/.main-content-->
</div><!--/.main-container-->



{%endblock%}